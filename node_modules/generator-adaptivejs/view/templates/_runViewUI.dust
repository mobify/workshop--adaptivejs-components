{<uiScripts}
    <script>
    // Note: Add any JavaScript that should be run on this page into
    // app/pages/<%= viewName %>/<%= viewName %>-ui.js
    (function(require) {
        require(["global/ui", "pages/<%= viewName %>/<%= viewName %>-ui"], function(globalUI, <%= uiFunction %>UI) {
            globalUI();
            <%= uiFunction %>UI();
        });
    })(Adaptive.AMD.require);
    </script>
{/uiScripts}
